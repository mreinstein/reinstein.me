{
  "name": "reinstein.me",
  "version": "0.0.2",
  "description": "https://reinstein.me",
  "private": true,
  "scripts": {
    "build-local": "rollup -c rollup.config.js index.js --file bundle.js --format iife",
    "deploy": "npm run build-local && now --static && now alias",
    "start": "serve ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreinstein/reinstein.me.git"
  },
  "author": "Mike Reinstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mreinstein/reinstein.me/issues"
  },
  "homepage": "https://github.com/mreinstein/reinstein.me#readme",
  "dependencies": {
    "serve": "^11.0.0",
    "text-animate": "^0.9.0"
  },
  "devDependencies": {
    "rollup": "^2.0.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "now": {
    "alias": [
      "reinstein.me",
      "www.reinstein.me"
    ]
  }
}
